{
    "version": 2,
    "builds": [
      {
        "src": "frontend/Turf/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/Turf/dist"  
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/Turf/dist/$1"  
      }
    ]
  }
  